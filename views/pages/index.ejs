<!DOCTYPE HTML>
<html>

<head>
    <%- include('../layout/head') %>
</head>

<body>
    <header>
        <%- include('../layout/header') %>
    </header>
    <main>
        <section class="book">
            <h2 class="headerDiv">
                <%= books.length %> Saved Books
            </h2>
            <ul>
                <% books.forEach(book => {%>
                <li class="bookResult">
                    <%- include('books/detail', { book:book, page }) %>
                </li>
                <%}) %>
            </ul>
        </section>
    </main>
    <footer>
        <%- include('../layout/footer') %>
    </footer>
</body>

</html>